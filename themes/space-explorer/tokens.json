{
  "name": "Space Explorer",
  "description": "Dark cosmic theme with deep blues, glowing accents, and starfield atmosphere",
  "colors": {
    "primary": "#6C5CE7",
    "primary-dark": "#5A4BD1",
    "primary-light": "#A29BFE",
    "secondary": "#0EA5E9",
    "accent": "#FFD93D",
    "background": "#0A1628",
    "surface": "#1A2744",
    "surface-elevated": "#243352",
    "text": "#FFFFFF",
    "text-secondary": "#B2BEC3",
    "text-on-dark": "#FFFFFF",
    "border": "rgba(255,255,255,0.1)",
    "border-subtle": "rgba(255,255,255,0.05)",
    "success": "#00B894",
    "success-bg": "rgba(0,184,148,0.15)",
    "error": "#E74C3C",
    "error-bg": "rgba(231,76,60,0.15)",
    "warning": "#F39C12",
    "warning-bg": "rgba(243,156,18,0.15)",
    "info": "#0EA5E9",
    "info-bg": "rgba(14,165,233,0.15)",
    "growth": "#A29BFE",
    "growth-bg": "rgba(162,155,254,0.15)"
  },
  "fonts": {
    "heading": "'Tajawal', sans-serif",
    "body": "'Tajawal', sans-serif",
    "mono": "'Fira Code', monospace",
    "files": ["tajawal-regular.woff2", "tajawal-bold.woff2"]
  },
  "typography": {
    "display": { "size": "2.5rem", "weight": 700 },
    "h1": { "size": "2rem", "weight": 700 },
    "h2": { "size": "1.625rem", "weight": 700 },
    "h3": { "size": "1.375rem", "weight": 600 },
    "body": { "size": "1.125rem", "weight": 400 }
  },
  "spacing": { "xs": "4px", "sm": "8px", "md": "16px", "lg": "24px", "xl": "32px" },
  "radius": { "sm": "8px", "md": "12px", "lg": "20px" },
  "shadows": {
    "sm": "0 2px 8px rgba(0,0,0,0.3)",
    "md": "0 4px 16px rgba(0,0,0,0.4)",
    "lg": "0 10px 40px rgba(0,0,0,0.5)"
  },
  "motion": { "fast": "150ms ease", "normal": "300ms ease", "slow": "500ms ease" }
}
