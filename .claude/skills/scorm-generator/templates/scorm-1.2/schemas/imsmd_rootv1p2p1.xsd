<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:imsmd="http://www.imsglobal.org/xsd/imsmd_rootv1p2p1"
            targetNamespace="http://www.imsglobal.org/xsd/imsmd_rootv1p2p1"
            elementFormDefault="qualified"
            version="1.2.1">

  <xsd:element name="lom">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="general" minOccurs="0">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="title" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="langstring" type="xsd:string"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="description" minOccurs="0">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="langstring" type="xsd:string"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
              <xsd:element name="keyword" minOccurs="0" maxOccurs="unbounded">
                <xsd:complexType>
                  <xsd:sequence>
                    <xsd:element name="langstring" type="xsd:string"/>
                  </xsd:sequence>
                </xsd:complexType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

</xsd:schema>
